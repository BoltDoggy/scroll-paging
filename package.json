    "url": "https://github.com/BoltDoggy/scroll-paging/issues"
  },
  "homepage": "https://github.com/BoltDoggy/scroll-paging#readme",
  "devDependencies": {
    "@vue/component-compiler-utils": "^1.3.1",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "parcel-bundler": "^1.8.1",
    "vue": "^2.5.16",
    "vue-hot-reload-api": "^2.3.0",
    "vue-template-compiler": "^2.5.16"
  },
  "dependencies": {}
